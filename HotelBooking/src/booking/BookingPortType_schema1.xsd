<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema version="1.0" targetNamespace="http://booking/" xmlns:tns="http://booking/" xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <xs:element name="bookRoom" type="tns:bookRoom"/>

  <xs:element name="bookRoomResponse" type="tns:bookRoomResponse"/>

  <xs:element name="cancelBooking" type="tns:cancelBooking"/>

  <xs:element name="cancelBookingResponse" type="tns:cancelBookingResponse"/>

  <xs:element name="sayHelloWorldFrom" type="tns:sayHelloWorldFrom"/>

  <xs:element name="sayHelloWorldFromResponse" type="tns:sayHelloWorldFromResponse"/>

  <xs:element name="validatePayment" type="tns:validatePayment"/>

  <xs:element name="validatePaymentResponse" type="tns:validatePaymentResponse"/>

  <xs:complexType name="validatePayment">
    <xs:sequence>
      <xs:element name="arg0" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="validatePaymentResponse">
    <xs:sequence>
      <xs:element name="return" type="xs:boolean"/>
